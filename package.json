{
  "name": "lewis",
  "version": "0.2.1",
  "description": "Syntax agnostic compiler framework",
  "main": "bin/index.js",
  "scripts": {
    "build": "NODE_ENV=production babel src --out-dir bin",
    "unit": "mocha test/unit --require babel-polyfill --require babel-register",
    "fixture": "mocha test/fixture --require babel-polyfill --require babel-register",
    "test": "npm run unit && npm run fixture",
    "preversion": "npm test",
    "version": "git checkout master && git merge dev",
    "postversion": "npm run build && npm publish && git push origin master --tags && git checkout dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hyeonupark/lewis.git"
  },
  "keywords": [
    "compiler"
  ],
  "author": "HyeonuPark",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hyeonupark/lewis/issues"
  },
  "homepage": "https://github.com/hyeonupark/lewis#readme",
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "mocha": "^2.4.5"
  },
  "dependencies": {
    "immutable": "^3.8.1",
    "iterator-util": "^1.4.1",
    "type-detect": "^2.0.0"
  }
}
